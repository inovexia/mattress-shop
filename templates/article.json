    "custom_liquid_RCgqzq": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<!-- Add this at the top of your blog template -->\n<div id=\"table-of-contents\"><\/div>\n\n<!-- Add this where you want the actual table of contents to appear -->\n{% if article.content != blank %}\n  <script>\n    document.addEventListener('DOMContentLoaded', function() {\n      var tocContainer = document.getElementById('table-of-contents');\n      var headings = document.querySelectorAll('.rte h2, .rte h3, .rte h4, .rte h5, .rte h6');\n\n      if (headings.length > 1) {\n        tocContainer.innerHTML = '<h2>Table of Contents<\/h2><ul>';\n        headings.forEach(function(heading) {\n          var anchor = heading.id ? heading.id : heading.textContent.toLowerCase().replace(\/\\s+\/g, '-');\n          tocContainer.innerHTML += '<li><a href=\"#' + anchor + '\">' + heading.textContent + '<\/a><\/li>';\n          heading.id = anchor;\n        });\n        tocContainer.innerHTML += '<\/ul>';\n      }\n    });\n  <\/script>\n{% endif %}",
        "color_scheme": "background-1",
        "padding_top": 40,
